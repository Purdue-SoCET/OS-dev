CAPI=2:
name: socet:OS-dev:crc

filesets:
    rtl:
        files:
            - CRC/crc.sv
        file_type: systemVerilogSource


targets:
    default: &default
        filesets:
            - rtl
        toplevel: crc

    fpga:
        <<: *default
        description: Synthesize for FPGA
        default_tool: quartus
        toplevel:
            - crc

        tools:
            quartus:
                family: Cyclone IV E
                device: EP4CE115F29C7 
                
